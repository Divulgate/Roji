{
  "name": "game",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "Modules": {
        "$path": "src/modules"
      },
      "Packages": {
        "$path": "Packages"
      },
      "Assets": {
      	"$path": "assets"
      }
    },

    "ServerScriptService": {
      "Controller": {
        "$path": "src/server"
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "Controller": {
          "$path": "src/client"
        }
      }
    },

    "ServerStorage": {
      "Modules": {
        "$path": "src/servermodules"
      }
    },
    
    "Workspace": {
    	"$path": "map/Workspace.rbxm"
    },
    
    "Lighting": {
    	"$path": "map/Lighting.rbxm"
    },

    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}